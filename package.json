{
  "name": "hot7s",
  "private": true,
  "version": "0.1.0",
  "author": "FalconPilot",
  "repository": "https://github.com/FalconPilot/hot7s",
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "yarn run clean && tsc --project tsconfig.json && cp -r static dist/static && yarn run paths",
    "paths": "tscpaths -p tsconfig.json -s . -o ./dist",
    "pretty": "prettier --write ./**/*.ts",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "configparser": "^0.3.8"
  },
  "devDependencies": {
    "electron": "^11.2.1",
    "prettier": "^2.2.1",
    "tscpaths": "^0.0.9",
    "typescript": "^4.1.3"
  }
}
